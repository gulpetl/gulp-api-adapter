{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "GulpTester",
            "type": "node",
            "request": "launch",
            "program": "${workspaceRoot}/node_modules/gulp/bin/gulp.js",
            "stopOnEntry": false,
            "args": ["--gulpfile", "./debug/gulpfile.ts", "default"],
            "cwd": "${workspaceRoot}",
            "runtimeArgs": [
                "--nolazy", "-r", "ts-node/register"
            ],
            "console": "internalConsole",
            "env": {"DEBUG_LEVEL":"info"}
        },
        {        
            "name": "GulpTester Debug",
            "type": "node",
            "request": "launch",
            "program": "${workspaceRoot}/node_modules/gulp/bin/gulp.js",
            "stopOnEntry": false,
            "args": ["--gulpfile", "./debug/gulpfile.ts", "default"],
            "cwd": "${workspaceRoot}",
            "runtimeArgs": [
                "--nolazy", "-r", "ts-node/register"
            ],
            "console": "internalConsole",
            "env": {"DEBUG_LEVEL":"debug"}
        },
        {        
            "name": "GulpTester Debug - Buffer Mode",
            "type": "node",
            "request": "launch",
            "program": "${workspaceRoot}/node_modules/gulp/bin/gulp.js",
            "stopOnEntry": false,
            "args": ["--gulpfile", "./debug/gulpfile.ts", "runtargetCsvBuffer"],
            "cwd": "${workspaceRoot}",
            "runtimeArgs": [
                "--nolazy", "-r", "ts-node/register"
            ],
            "console": "internalConsole",
            "env": {"DEBUG_LEVEL":"debug"}
        },
        {
            "name": "csvStringifyWithoutGulp",
            "type": "node",
            "request": "launch",
            "program": "${workspaceRoot}/node_modules/gulp/bin/gulp.js",
            "stopOnEntry": false,
            "args": ["--gulpfile", "./debug/gulpfile.ts", "csvStringifyWithoutGulp"],
            "cwd": "${workspaceRoot}",
            "runtimeArgs": [
                "--nolazy", "-r", "ts-node/register"
            ],
            "console": "internalConsole",
            "env": {"DEBUG_LEVEL":"info"}
        },
        {
            "name": "aws-doParse: debug TS w/o transpile using ts-node",
            "type": "node",
            "request": "launch",
            "args": ["src/aws/dev-handler.ts"],
            "runtimeArgs": ["-r", "ts-node/register"],
            "cwd": "${workspaceRoot}",
            "protocol": "inspector",
            "internalConsoleOptions": "openOnSessionStart",
        },
    ]
}