{
  "name": "gulp-api-adapter",
  "version": "0.0.5",
  "repository": "https://github.com/gulpetl/gulp-api-adapter",
  "description": "Upload/download files to/from REST APIs (or any other http/https endpoints)",
  "main": "dist/js/plugin.js",
  "typings": "dist/types/plugin.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
