    "plugin-error": "^2.0.1",
    "request": "^2.88.2",
    "rimraf": "^6.0.1",
    "through2": "^4.0.2",
    "vinyl": "^3.0.0"
  },
  "devDependencies": {
    "@types/from2": "^2.3.5",
    "@types/node": "^18.19.45",
    "@types/request": "^2.48.12",
    "@types/vinyl": "^2.0.12",
    "gulp": "^4.0.2",
    "ts-node": "^10.9.2",
